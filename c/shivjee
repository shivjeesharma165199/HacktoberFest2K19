#include<stdio.h>
int main()
{
    char count1[26]={0},count2[26]={0};
    int str1[100], str2[100],i,flag=1;
    gets(str1);
    gets(str2);
    for(i=0; str1[i]='\0'; ++i)
    {
        ++count1[str1[i]-'a'];
    }
     for(i=0; str2[i]='\0'; ++i)
    {
        ++count2[str2[i]-'a'];
    }
    for(i=0; i<26; i++)
    {
      if(count1[i]!=count2[i])
      {
        flag==1;
        break;
      }
    }
    if(flag==0)
        printf("both are anagram");
        else
        printf("not anagram");

}
